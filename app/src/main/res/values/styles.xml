<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="FullScreenStyle" parent="AppTheme">
        <!--去掉状态栏的设置-->
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="main_tab_rb_style">
        <item name="android:layout_width">@dimen/dp_0</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">@dimen/dp_2</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/selector_main_top_tab_textcolor</item>
    </style>
</resources>